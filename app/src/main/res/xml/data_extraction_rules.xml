<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive auth data from backup -->
        <exclude domain="sharedpref" path="aquaglow_prefs" subpath="auth_"/>
        <exclude domain="sharedpref" path="aquaglow_prefs" subpath="user_password"/>
        <exclude domain="sharedpref" path="aquaglow_prefs" subpath="password_"/>
        <exclude domain="sharedpref" path="aquaglow_prefs" subpath="login_"/>
        <exclude domain="sharedpref" path="aquaglow_prefs" subpath="session_"/>
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>